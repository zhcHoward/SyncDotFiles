{
    "configs": {
        "vim": {
            "paths": [".vimrc", ".vim/autoload"]
        },
        "zsh": {
            "paths": [".zshrc"]
        },
        "tmux": {
            "paths": [".tmux.conf"]
        },
        "sublime-text": {
            "path_prefix": ".config/sublime-text-3/Packages/User/",
            "paths": [
                ".config/sublime-text-3/Packages/User/Anaconda.sublime-settings",
                ".config/sublime-text-3/Packages/User/divider.sublime-snippet",
                ".config/sublime-text-3/Packages/User/Doxy.sublime-settings",
                ".config/sublime-text-3/Packages/User/Preferences.sublime-settings",
                ".config/sublime-text-3/Packages/User/Package Control.sublime-settings"
            ]
        }
    },
    "local": {
        "home": "/home/howard/"
    },
    "cache": {
        "base": "/home/howard/"
    },
    "git_repo": "git@github.com:zhcHoward/.mydotfiles.git"
}
